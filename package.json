{
  "name": "schedulo",
  "private": true,
  "packageManager": "pnpm@8.15.9",
  "scripts": {
    "dx:next": "next dev",
    "dx": "run-p dx:* --print-label",
    "dev": "pnpm dx:next",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --cache src",
    "lint-fix": "pnpm lint --fix",
    "typecheck": "tsc --noEmit",
    "test": "pnpm prettier:check && pnpm lint && pnpm typecheck",
    "prettier:check": "prettier --check \"**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \"**/*.{ts,tsx}\""
  },
  "prettier": {
    "printWidth": 80,
    "trailingComma": "all",
    "singleQuote": true
  },
  "dependencies": {
    "@mantine/core": "^8.3.10",
    "@mantine/dates": "^8.3.10",
    "@mantine/form": "^8.3.10",
    "@mantine/hooks": "^8.3.10",
    "@mantine/modals": "^8.3.10",
    "@mantine/notifications": "^8.3.10",
    "@mantine/nprogress": "^8.3.10",
    "@tabler/icons-react": "^3.36.0",
    "@tanstack/react-query": "^5.80.3",
    "@trpc/client": "npm:@trpc/client",
    "@trpc/next": "npm:@trpc/next",
    "@trpc/react-query": "npm:@trpc/react-query",
    "@trpc/server": "npm:@trpc/server",
    "bcryptjs": "^3.0.3",
    "clsx": "^2.0.0",
    "cookie": "^1.1.1",
    "dayjs": "^1.11.19",
    "next": "^15.3.8",
