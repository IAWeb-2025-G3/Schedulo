    "@types/node": "^22.13.5",
    "@types/react": "^19.1.0",
    "autoprefixer": "^10.4.7",
    "dotenv": "^16.0.1",
    "eslint": "^9.26.0",
    "eslint-config-next": "^15.3.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "6.0.0-rc.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "postcss-preset-mantine": "^1.18.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.6",
    "tailwindcss-debug-screens": "^2.2.1",
    "tsx": "^4.19.3",
    "typescript": "^5.9.2",
